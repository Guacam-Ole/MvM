// To create a new instance from the server, copy this file to secrets.json and enter the correct values
{
  "salt": "<salt>",  // Salt for hashes
  "admin": "<hash>", // Hashed adminpassword (required to change the upload-code). To create a new hash, simply enter "<salt>_<password>" into any online md5-hashgenerator
  "auphonic": {      // auphonic credentials
    "username": "<auphonicuser>",
    "password": "<auphonicpassword>"
  },
  "github": {       // github credentials (creating PRs)
    "username": "<githubuser>",
    "password": "<githubpassword>"
  }
}
